<?xml version="1.0" encoding="utf-8"?>
<wimygit_plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Plugin.xsd">
  <version>1</version>
  <name>GitCommand</name>
  <description>Sample GitCommand</description>
  <title>GitCommand</title>
  <icon>
    <type>plugin_directory</type>
    <path>GitCommand.png</path>
  </icon>
  <command>git</command>
  <arguments>
    <argument>
      <type>string</type>
      <value>status --untracked-files=all</value>
    </argument>
  </arguments>
  <execution_type>WimyGitInnerShellAndRefreshRepositoryStatus</execution_type>
</wimygit_plugin>
